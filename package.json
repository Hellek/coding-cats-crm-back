{
	"name": "coding-cats-crm-back",
	"description": "Designed by Coding cats",
	"version": "1.0.0",
	"private": false,
	"main": "dist/index.js",
	"author": {
		"name": "Roman Basharin",
		"email": "my@hellek.ru"
	},
	"license": "ISC",
	"engines": {
		"node": ">= 13.12.0",
		"npm": ">= 6.14.4"
	},
	"scripts": {
		"dev": "cross-env NODE_ENV=development rollup -c -w",
		"lint": "eslint src .eslintrc.js rollup.config.js --config .eslintrc.js",
		"lint:fix": "eslint src .eslintrc.js rollup.config.js --config .eslintrc.js --fix",
		"build": "cross-env NODE_ENV=production rollup -c",
		"npm:install": "npm install",
		"serve:build:prod": "cross-env NODE_ENV=production node dist/index.js",
		"serve:build:dev": "cross-env NODE_ENV=development node dist/index.js",
		"heroku:deploy": "git push heroku master --force",
		"heroku:logs": "heroku logs --tail",
		"heroku:pg:connect": "heroku pg:psql postgresql-animate-99818 --app coding-cats-crm-back",
		"start": "npm run build && npm run serve:build:prod"
	},
	"dependencies": {
		"@babel/core": "7.11.6",
		"@babel/plugin-proposal-optional-chaining": "7.11.0",
		"@koa/cors": "3.1.0",
		"@rollup/plugin-eslint": "^8.0.1",
		"@rollup/plugin-run": "2.0.2",
		"@tinkoff/invest-openapi-js-sdk": "1.2.16",
		"bcryptjs": "2.4.3",
		"cross-env": "7.0.2",
		"cryptr": "6.0.2",
		"dotenv": "8.2.0",
		"isemail": "3.2.0",
		"jsonwebtoken": "8.5.1",
		"koa": "2.13.0",
		"koa-bodyparser": "4.3.0",
		"koa-helmet": "6.0.0",
		"koa-jwt": "4.0.0",
		"koa-router": "9.4.0",
		"nodemailer": "6.4.14",
		"pg": "8.4.1",
		"rollup": "2.29.0",
		"rollup-plugin-babel": "4.4.0",
		"socket.io": "3.0.0"
	}
}
